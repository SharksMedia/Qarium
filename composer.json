{
    "name": "sharksmedia/qarium",
    "description": "A objectionjs inspired ORM",
    "type": "library",
    "license": "MIT",
    "version": "0.0.1",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "platform": {

        }
    },
    "scripts": {
        "unit-tests": "./vendor/bin/codecept run unit",
        "integration-tests": "./vendor/bin/codecept run integration",
        "codecept-build": "./vendor/bin/codecept build"
    },
    "scripts-descriptions": {
        "unit-tests": "Run unit tests"
    },
    "autoload": {
        "psr-4": {
            "Sharksmedia\\Qarium\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Magnus S. Rasmussen",
            "email": "magnus@sharksmedia.dk"
        }
    ],
    "repositories": {
        "sharksmedia/sharq": {
            "type": "vcs",
            "url": "git@github.com:SharkMagnus/SharQ.git"
        },
        "sharksmedia/query-builder": {
            "type": "vcs",
            "url": "git@github.com:SharkMagnus/Qarium.git"
        }
    },
    "require-dev": {
        "codeception/codeception": "^5.0",
        "codeception/module-asserts": "*",
        "codeception/module-db": "^3.1",
        "friendsofphp/php-cs-fixer": "^3.34",
        "kubawerlos/php-cs-fixer-custom-fixers": "^3.16"
    },
    "require": {
        "sharksmedia/sharq": "master",
        "php": ">=7.4"
    }
}
